                <!--Grid with targeting elements-->
                <Grid x:Name="targeting_grid" Grid.Row="2" Grid.Column="0" Grid.RowSpan="2">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="0.25*"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                    </Grid.RowDefinitions>
                    <Label Content="Targeting" HorizontalContentAlignment="Center" FontSize="36" Foreground="White" Grid.Row="0" Margin="0,0,0,5" FontWeight="Bold" Panel.ZIndex="1" FontFamily="{StaticResource AVB}"/>
                    <ListBox x:Name="Targeting_list_display" Grid.Row="1" d:ItemsSource="{d:SampleData ItemCount=5}" FontSize="36" HorizontalContentAlignment="Center" Background="{x:Null}" Foreground="White" BorderBrush="Black" Panel.ZIndex="1" FontFamily="{StaticResource AS90}"/>
                    <Rectangle  Name="targetingrec" Grid.Row="0" Grid.RowSpan="2" Stroke="Black" Fill="Black" Panel.ZIndex="4" Opacity="0.0"  />

                </Grid>
                <!--Grid with attack elements-->
                <Grid x:Name="attak_grid" Grid.Column="1" Grid.Row="2" Grid.RowSpan="2">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="221*"></ColumnDefinition>
                        <ColumnDefinition Width="195*"></ColumnDefinition>
                        <ColumnDefinition Width="224*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <TextBlock x:Name="Attack1_timer" Grid.Row="0" TextWrapping="Wrap" Text="00:00:00" Foreground="White" TextAlignment="Center"   FontSize="30" Margin="11,22,0,10" Grid.Column="1" Panel.ZIndex="1" FontFamily="{StaticResource AS90}"  />
                    <TextBlock x:Name="Attack2_timer" Grid.Row="1" TextWrapping="Wrap" Text="00:00:00" Foreground="White" TextAlignment="Center"   FontSize="30" Margin="11,22,0,10" Grid.Column="1" Panel.ZIndex="1" FontFamily="{StaticResource AS90}" />
                    <TextBlock x:Name="Attack3_timer" Grid.Row="2" TextWrapping="Wrap" Text="00:00:00" Foreground="White" TextAlignment="Center"   FontSize="30" Margin="11,22,0,10" Grid.Column="1" Panel.ZIndex="1" FontFamily="{StaticResource AS90}" />
                    <TextBlock x:Name="Attack4_timer" Grid.Row="3" TextWrapping="Wrap" Text="00:00:00" Foreground="White" TextAlignment="Center"   FontSize="30" Margin="11,22,0,10" Grid.Column="1" Panel.ZIndex="1" FontFamily="{StaticResource AS90}" />

                    <Rectangle x:Name="Attack_border"   Grid.Row="0" Grid.RowSpan="4" Grid.Column="0" Grid.ColumnSpan="3"  Stroke="Black" Fill="Black" Panel.ZIndex="0" Opacity="0.0"/>
                    <Rectangle x:Name="Attack1_box"  Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Stroke="Black" Panel.ZIndex="0"   >
                    </Rectangle>
                    <TextBlock x:Name="Attack1_name" Grid.Column="0" Grid.Row="0" TextWrapping="Wrap" Text="Attack 1" Foreground="White" TextAlignment="Center"   FontSize="36" Margin="0,22,18,88" Grid.RowSpan="2" Panel.ZIndex="0" FontFamily="{StaticResource AS60}" />
                    <Rectangle x:Name="Attack2_box"  Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" Stroke="Black" Panel.ZIndex="0"  />
                    <TextBlock x:Name="Attack2_name" Grid.Column="0" Grid.Row="1" TextWrapping="Wrap" Text="Attack 2" Foreground="White" TextAlignment="Center"   FontSize="36" Margin="0,22,18,88" Grid.RowSpan="2" Panel.ZIndex="0" FontFamily="{StaticResource AS60}" />
                    <Rectangle x:Name="Attack3_box"  Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" Stroke="Black"  Panel.ZIndex="0" />
                    <TextBlock x:Name="Attack3_name" Grid.Column="0" Grid.Row="2" TextWrapping="Wrap" Text="Attack 3" Foreground="White" TextAlignment="Center"   FontSize="36" Margin="0,22,18,88" Grid.RowSpan="2" Panel.ZIndex="0" FontFamily="{StaticResource AS60}" />
                    <Rectangle x:Name="Attack4_box"  Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="2" Stroke="Black" Panel.ZIndex="0"  />
                    <TextBlock x:Name="Attack4_name" Grid.Column="0" Grid.Row="3" TextWrapping="Wrap" Text="Attack 4"  Foreground="White" TextAlignment="Center"   FontSize="36" Margin="0,20,18,16" Panel.ZIndex="0" FontFamily="{StaticResource AS60}" />



                    <Button x:Name="Attack1_button"  Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Opacity="0"   Panel.ZIndex="3"  Click="Attack1_button_Click"/>

                    <Button x:Name="Attack2_button"  Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" Opacity="0"   Panel.ZIndex="3" Click="Attack2_button_Click"/>

                    <Button x:Name="Attack3_button"  Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" Opacity="0"   Panel.ZIndex="3" Click="Attack2_button_Click"/>

                    <Button x:Name="Attack4_button"  Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="2" Opacity="0"   Panel.ZIndex="3" Click="Attack2_button_Click"/>
                    <Button x:Name="sendattack_button2" Grid.Column="2" Content="Attack" Grid.Row="0" Margin="24,52,40,10" Grid.RowSpan="3" Background="{x:Null}" Foreground="White" FontSize="25" BorderBrush="Black"  Panel.ZIndex="4" FontFamily="{StaticResource AVB}" Click="Button_Click" Opacity="0.0" IsEnabled="False"/>
                    <Rectangle Grid.Column="2" Grid.Row="0" Grid.RowSpan="4" Fill="Black" Panel.ZIndex="3" Opacity="0.6" Name="AttackbuttonRec"/>

                    <Button x:Name="sendattack_button" Grid.Column="2" Content="Attack" Grid.Row="0" Margin="24,52,40,10" Grid.RowSpan="3" Background="{x:Null}" Foreground="White" FontSize="25" BorderBrush="Black"  Panel.ZIndex="1" FontFamily="{StaticResource AVB}" Click="Button_Click"/>
                    
                </Grid>
                <!--Grid with adapted message items to display tutorial-->
                <Grid x:Name="message_grid" Grid.Column="2" Grid.Row="0">

                    <Grid.RowDefinitions>
                        <RowDefinition Height="0.2*"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                        <RowDefinition Height="0.25*"></RowDefinition>

                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition Width="0*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>

                    <Label Name="message_title" Content="Tutorial 3" HorizontalContentAlignment="Center" FontSize="36" Foreground="White" Grid.Row="0" Grid.Column="0" Margin="0,0,0,5" FontWeight="Bold" FontFamily="{StaticResource AVB}" />
                    <ListBox x:Name="messagedisplay_box" HorizontalContentAlignment="Center" d:ItemsSource="{d:SampleData ItemCount=5}" Background="{x:Null}" Foreground="White" FontSize="24" BorderBrush="Black" ScrollViewer.HorizontalScrollBarVisibility="Disabled" FontFamily="{StaticResource AS90}" Margin="0,73,0,0" Grid.RowSpan="2" >
                    </ListBox>
                    <Button Name="lastbutton" Content="Last"  HorizontalAlignment="Left" Height="78" Margin="163,3,0,0" VerticalAlignment="Top" Width="134" FontSize="35" Grid.Row="2" Click="Button_Click"/>
                    <Button x:Name="nextbutton" Content="Next"  HorizontalAlignment="Left" Height="78" Margin="328,4,0,0" VerticalAlignment="Top" Width="134" FontSize="35" Grid.Row="2" Click="Button_Click"/>

                </Grid>

                <!--Grid with stikefeed elements-->
                <Grid x:Name="strikefeed_grid" Grid.Row="1" Grid.Column="2" Grid.RowSpan="3">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="0.2*"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>

                    </Grid.RowDefinitions>

                    <Rectangle x:Name="strikefeed_box" Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" Grid.RowSpan="3"  Stroke="Black" Panel.ZIndex="3" Opacity="1">
                        <Rectangle.Fill>
                            <SolidColorBrush Color="Black" Opacity="0.6"/>
                        </Rectangle.Fill>
                    </Rectangle>
                    <Label x:Name="stikefeed_title" Content="Strike Feed" HorizontalContentAlignment="Center" FontSize="36" Foreground="White" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Margin="0,0,0,5" FontWeight="Bold" FontFamily="{StaticResource AVB}" />
                    <ListBox x:Name="stikefeeddisplay_box" Grid.ColumnSpan="2" Grid.Row="1" HorizontalContentAlignment="Center"   d:ItemsSource="{d:SampleData ItemCount=5}" Background="{x:Null}" Foreground="White" FontSize="24" BorderBrush="Black" FontFamily="{StaticResource AS90}" />


                </Grid>




            </Grid>

        </Grid>
    </Grid>
</Window>
